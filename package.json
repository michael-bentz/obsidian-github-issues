		"builtin-modules": "^5.0.0",
		"esbuild": "^0.25.5",
		"eslint": "^9.29.0",
		"obsidian": "latest",
		"tslib": "^2.8.1",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"date-fns": "^4.1.0",
